[ignore]
# Ignore all temp files
.*/tmp

# Flow error on line 53 (for both files)
.*/node_modules/redux-persist/lib/index.js.flow
.*/node_modules/redux-persist/es/index.js.flow

# Flow error on line 406
.*/node_modules/immutable/dist/immutable.js.flow

# Flow tries to check some of the ejs templates
